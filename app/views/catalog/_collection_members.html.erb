<% document ||= @document %>

<% if document.collection_members.present? %>
  <% if document.collection_members.render_type == 'filmstrip' %>
    <%= render partial: "catalog/image_collection_filmstrip", locals: { collection_document: document } %>
  <% else %>
    <%= render partial: 'catalog/file_collection_members', locals: { document: document } %>
  <% end %>
<% end %>
